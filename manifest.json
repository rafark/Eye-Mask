{
  "name": "Eye Mask",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Hide all virus-related threads on BlackHatWorld.com",
  "homepage_url": "https://www.blackhatworld.com/members/rafark.631843/",
  "icons": {
  },
  "default_locale": "en",
  "permissions": [
    "contentSettings"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.blackhatworld.com/*"
      ],
      "js": [
        "src/inject/remover.js"
      ]
    }
  ]
}